    "unlimitedStorage","notifications","contentSettings","downloads",
    "clipboardWrite","clipboardRead",
    "nativeMessaging",
    "webRequest",
    "webRequestBlocking",
    "https://b08-admin.shop6888.com/#/goods/goodsManagement/*"
