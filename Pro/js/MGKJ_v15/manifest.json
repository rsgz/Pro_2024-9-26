        "https://www.wish.com/*",
        "https://*.wish.com/*",
        "https://web.baimiaoapp.com/*",
        "https://www.bilibili.com/*",
        "https://www.douyin.com/*",
        "https://seller.shopee.cn/*",
        "https://*.shopee.cn/*",
        "https://*.xiapibuy.com/*",
        "https://mobile.yangkeduo.com/*",
