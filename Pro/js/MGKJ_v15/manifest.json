    "http://www.rsgz.top/*",
    "https://chat.chatgptdemo.net/*",
    "http://www.360doc.com/*",
    "https://*.mangoerp.com/*",
    "https://*.shop6888.com/*",
    "https://cdn.tailwindcss.com/*",
    "https://*/*"
